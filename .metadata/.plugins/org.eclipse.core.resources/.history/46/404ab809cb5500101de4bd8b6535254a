package com.problems.array;

import java.util.HashSet;
import java.util.Set;

public class First_Repeating_Element 
{
	private static Integer first_Repeating_Element(int nums[])
	{
		Set<Integer> set=new HashSet<>();
		
		for(int num:nums)
		{
			if(set.contains(num))
			{
				return num;
			}
		}
		
		return null;
	}

}
